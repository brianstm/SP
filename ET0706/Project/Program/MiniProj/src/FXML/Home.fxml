<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #FFFFFF;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.HomeController">
   <children>
      <VBox prefHeight="400.0" prefWidth="135.0" spacing="5.0" style="-fx-background-color: #28b498;">
         <children>
            <Button fx:id="home" mnemonicParsing="false" prefHeight="40.0" prefWidth="210.0" style="-fx-background-color: #298c79; -fx-text-fill: #FFFFFF; -fx-alignment: center-left;" text="Home">
               <font>
                  <Font name="DM Sans Bold" size="10.0" />
               </font>
            </Button>
            <Button fx:id="goVote" mnemonicParsing="false" onAction="#onClickVote" prefHeight="40.0" prefWidth="189.0" style="-fx-background-color: #28b498; -fx-text-fill: #FFFFFF; -fx-alignment: center-left;" text="Votes">
               <font>
                  <Font name="DM Sans Bold" size="10.0" />
               </font>
            </Button>
            <Button fx:id="goFinalVotes" mnemonicParsing="false" onAction="#onClickFinalVotes" prefHeight="40.0" prefWidth="210.0" style="-fx-background-color: #28b498; -fx-text-fill: #FFFFFF; -fx-alignment: center-left;" text="See final votes">
               <font>
                  <Font name="DM Sans Bold" size="10.0" />
               </font>
            </Button>
         </children>
         <padding>
            <Insets bottom="30.0" top="30.0" />
         </padding>
      </VBox>
      <Label fx:id="welcomeBack" layoutX="155.0" layoutY="18.0" text="Welcome back, ">
         <font>
            <Font name="DM Sans Bold" size="32.0" />
         </font>
      </Label>
      <Label fx:id="votingStatus" layoutX="155.0" layoutY="138.0" prefHeight="26.0" prefWidth="359.0" text="Voting status: ">
         <font>
            <Font name="DM Sans Bold" size="20.0" />
         </font>
      </Label>
      <Label fx:id="votedFor" layoutX="155.0" layoutY="164.0" prefHeight="26.0" prefWidth="340.0" text="Voted for: ">
         <font>
            <Font name="DM Sans Bold" size="20.0" />
         </font>
      </Label>
      <Label fx:id="DOBLabel" layoutX="155.0" layoutY="61.0" prefHeight="26.0" prefWidth="426.0" text="Date of Birth: ">
         <font>
            <Font name="DM Sans Bold" size="20.0" />
         </font>
      </Label>
      <Label fx:id="AgeLabel" layoutX="155.0" layoutY="86.0" prefHeight="26.0" prefWidth="340.0" text="Age: ">
         <font>
            <Font name="DM Sans Bold" size="20.0" />
         </font>
      </Label>
      <Button fx:id="logout" layoutX="25.0" layoutY="352.0" mnemonicParsing="false" onAction="#onClickLogout" prefHeight="30.0" prefWidth="83.0" style="-fx-background-color: transparent; -fx-border-radius: 20; -fx-text-fill: #FFFFFF; -fx-border-color: #FFFFFF; -fx-border-width: 2;" text="Logout">
         <font>
            <Font name="DM Sans Bold" size="10.0" />
         </font>
      </Button>
      <Label fx:id="eligibilityLabel" layoutX="155.0" layoutY="112.0" prefHeight="26.0" prefWidth="340.0" text="Voting Eligibility: ">
         <font>
            <Font name="DM Sans Bold" size="20.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
